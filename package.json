{
  "name": "@vtex/address-form",
  "version": "2.0.4",
  "description": "address-form React component",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "nwb build-react-component --no-demo && npm run removeMocks && npm run addLocales",
    "build:link": "npm link && watch 'npm run build' src",
    "removeMocks": "rm -rf lib/**/__mocks__ && rm -rf lib/__mocks__",
    "addLocales": "cp -R -f src/locales/ lib/locales/",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "jest --env=jsdom",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:watch": "jest --env=jsdom --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@vtex/styleguide": "^5.4.1",
    "axios": "^0.16.2",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "classnames": "^2.2.5",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "load-google-maps-api": "^1.0.0",
    "lodash": "^4.17.4",
    "msk": "1.0.3",
    "recompose": "^0.27.1"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "15.x || 16.x",
    "react-intl": "^2.4.0",
    "vtex-tachyons": "^2.5.0"
  },
  "devDependencies": {
    "babel-eslint": "7",
    "babel-jest": "^20.0.3",
    "babel-preset-react-app": "^3.0.1",
    "enzyme": "^3.4.1",
    "eslint": "3",
    "eslint-config-vtex": "5.1.0",
    "eslint-config-vtex-react": "3.0.2",
    "eslint-config-vtex-react-app": "^1.0.4",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5",
    "eslint-plugin-react": "7",
    "i18n-iso-countries": "^1.13.0",
    "jest": "^22.0.4",
    "jest-cli": "^22.4.3",
    "jest-enzyme": "^6.0.2",
    "nwb": "^0.20.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "next",
    "react-intl": "^2.3.0",
    "react-test-renderer": "^16.4.1",
    "watch": "^1.0.2"
  },
  "author": "Breno Calazans (@klzns)",
  "homepage": "https://github.com/vtex/address-form",
  "license": "MIT",
  "repository": "https://github.com/vtex/address-form",
  "keywords": [
    "react-component",
    "vtex",
    "checkout",
    "address"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/es/",
      "<rootDir>/lib/",
      "<rootDir>/umd/",
      "<rootDir>/react/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/es/",
      "<rootDir>/lib/",
      "<rootDir>/umd/",
      "<rootDir>/react/"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]lib[/\\\\].+\\.(js|jsx)$",
      "[/\\\\]umd[/\\\\].+\\.(js|jsx)$",
      "[/\\\\]es[/\\\\].+\\.(js|jsx)$",
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)(\\?.*)?$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    }
  }
}
